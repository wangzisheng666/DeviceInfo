<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="?attr/xui_config_color_background"
    android:orientation="vertical">

    <com.scwang.smartrefresh.layout.SmartRefreshLayout style="@style/PullDownStyle">

        <androidx.core.widget.NestedScrollView style="@style/ScrollViewStyle">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingTop="20dp">

                <com.xuexiang.xui.widget.textview.supertextview.SuperTextView
                    style="@style/InfoItem.Account1"
                    android:id="@+id/open_frida"
                    app:sLeftTextString="Frida"
                    app:sRightViewType="switchBtn"
                    app:sSwitchIsChecked="true"
                    app:sThumbResource="@drawable/thumb"
                    app:sTrackResource="@drawable/track"
                    />

                <com.xuexiang.xui.widget.textview.supertextview.SuperTextView
                    style="@style/InfoItem.Account1"
                    android:id="@+id/open_frida_big"
                    app:sLeftTextString="Frida隐藏版"
                    app:sRightViewType="switchBtn"
                    app:sSwitchIsChecked="true"
                    app:sThumbResource="@drawable/thumb"
                    app:sTrackResource="@drawable/track"/>



                <com.xuexiang.xui.widget.textview.supertextview.SuperTextView
                    android:id="@+id/open_debugable"
                    style="@style/InfoItem.Account1"
                    android:layout_marginTop="20dp"
                    app:sCenterTextString="全局调试"
                    app:sLeftTextString="ro.debugable"
                    app:sRightViewType="switchBtn"
                    app:sSwitchIsChecked="true"
                    app:sThumbResource="@drawable/thumb"
                    app:sTrackResource="@drawable/track"/>

                <com.xuexiang.xui.widget.textview.supertextview.SuperTextView
                    android:id="@+id/open_1"
                    style="@style/InfoItem.Account"
                    app:sDividerLineType="none"
                    app:sLeftTextString="test" />

            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </com.scwang.smartrefresh.layout.SmartRefreshLayout>

</LinearLayout>